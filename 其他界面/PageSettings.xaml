<Page x:Class="Software.其他界面.PageSettings"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Software.其他界面"
      xmlns:resx="clr-namespace:Software.Properties" 
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageSettings" Loaded="Page_Loaded">

    <Grid Background="Snow">
        <StackPanel>
            
            <TextBlock Text="是否开启启动计数" FontSize="24"/>
            <CheckBox DockPanel.Dock="Top" Foreground="Black"
          Name="EnableCountingCheckBox" 
          Content="启用计数" 
          Checked="EnableCountingCheckBox_Checked" 
          Unchecked="EnableCountingCheckBox_Unchecked"/>
        </StackPanel>
        
    </Grid>
</Page>
