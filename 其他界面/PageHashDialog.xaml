<Page x:Class="Software.其他界面.PageHashDialog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Software.其他界面"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageHashDialog">

    <Grid>
        <ScrollViewer>
            <StackPanel>
                <TextBox x:Name="FileTextBox" IsReadOnly="True" TextWrapping="Wrap" />
                <TextBox x:Name="MD5TextBox" IsReadOnly="True" TextWrapping="Wrap" />
                <TextBox x:Name="SHA1TextBox" IsReadOnly="True" TextWrapping="Wrap" />
                <TextBox x:Name="SHA256TextBox" IsReadOnly="True" TextWrapping="Wrap" />
                <TextBox x:Name="SHA384TextBox" IsReadOnly="True" TextWrapping="Wrap" />
                <TextBox x:Name="SHA512TextBox" IsReadOnly="True" TextWrapping="Wrap" />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
